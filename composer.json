{
    "name": "taxusorg/filesystem-qiniu",
    "description": "flysystem Adapter, Qiniu",
    "keywords": [
        "filesystem", "filesystems", "file systems", "files", "storage", "cloud", "cloud files", "qiniu"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taxusorg",
            "email": "taxus@taxus.org"
        }
    ],
    "require": {
        "qiniu/php-sdk": "^7.2",
        "league/flysystem": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Taxusorg\\FilesystemQiniu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Taxusorg\\FilesystemQiniu\\FilesystemServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "v1.1.x-dev"
        }
    }
    
}
